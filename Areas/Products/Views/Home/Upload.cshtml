@model Omni_MVC_2.Areas.Products.Models.FileUploadModel

<h2>Upload File</h2>

<form asp-controller="Home" asp-action="Upload" asp-area="Products" method="post" enctype="multipart/form-data">
    <div>
        <label for="File">Choose file:</label>
        <input type="file" id="File" name="File" />
    </div>
    <button type="submit">Upload</button>
</form>

@if (ViewBag.Message != null)
{
    <p style="color:green">@ViewBag.Message</p>
}
